{
  "name": "csanchez-skype",
  "version": "1.0.3",
  "author": "Carlos Sanchez",
  "summary": "Installs Skype",
  "license": "Apache-2.0",
  "source": "http://github.com/carlossg/puppet-skype",
  "project_page": "http://github.com/carlossg/puppet-skype",
  "dependencies": [
    {
      "name": "maestrodev/wget",
      "version_requirement": ">=1.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=3.2.0"
    },
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">=1.0.0"
    }
  ],
  "data_provider": "hiera",
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "10.04",
        "12.04",
        "14.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 3.0.0"
    }
  ],
  "description": "A Puppet module to install and configure Skype."
}
